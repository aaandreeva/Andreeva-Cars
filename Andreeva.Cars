    "def get_price_cat(Price):\n",
    "    if Price >= 5000000:\n",
    "        return 'high'\n",
    "    elif Price >= 800000 and Price < 5000000:\n",
    "        return 'medium'\n",
    "    elif Price < 800000:\n",
    "        return 'low'\n",
    "    else:\n",
    "        return Price"
   ]
